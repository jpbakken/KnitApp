#:kivy 1.8.0


<AppScreen>:

    MDTopAppBar:
        id: toolbar
        title: 'Projects'
        pos_hint: {'center_x':0.5, 'top':1}

    ScreenManager:
        id: sm
        ListScreen:
        TableScreen:



<ListScreen>:
    name: 'list'
    
    MDBoxLayout:  
        id: box
        size_hint: (.9, .6)
        pos_hint: {'center_x':.5, 'center_y':.5}
        
        ScrollView:
            MDList:
                id: list      


<TableScreen>:
    name: 'table'

    MDBoxLayout:  
        id:box
        size_hint: (.9, .6)
        pos_hint: {'center_x':.5, 'center_y':.5}
                        


